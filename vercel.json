{
  "version": 2,
  "builds": [{ "src": "app.py", "use": "@vercel/python" }],
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python",
      "memory": 512,
      "maxDuration": 60,
      "includeFiles": ["app.py", "requirements.txt"]
    }
  },
  "routes": [{ "src": "/(.*)", "dest": "/api/app.py" }]
}
